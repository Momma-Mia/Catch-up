<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/addFamilyFragment"
        android:name="dgsw.hackathon.catch_up.feature.chooserole.ChooseRoleFragment"
        android:label="fragment_add_family"
        tools:layout="@layout/fragment_choose_role" >
        <action
            android:id="@+id/action_addFamilyFragment_to_addFamilyFragment2"
            app:destination="@id/addFamilyFragment2" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="dgsw.hackathon.catch_up.feature.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_addFamilyFragment"
            app:destination="@id/addFamilyFragment" />
    </fragment>
    <fragment
        android:id="@+id/addFamilyFragment2"
        android:name="dgsw.hackathon.catch_up.feature.addfamily.AddFamilyFragment"
        android:label="fragment_add_family"
        tools:layout="@layout/fragment_add_family" />
</navigation>